<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:id="@+id/layoutBottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_999999"
        android:orientation="vertical"
        android:paddingTop="10dp"
        android:paddingBottom="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/btnBiz"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:text="@string/btn_biz_text"
                android:textColor="@color/white"
                android:textSize="11sp" />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_marginStart="6dip"
                android:layout_marginTop="3dip"
                android:layout_marginEnd="6dip"
                android:layout_marginBottom="3dip"
                android:background="@color/white" />

            <TextView
                android:id="@+id/btnUseTerms"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:text="@string/btn_use_terms_text"
                android:textColor="@color/white"
                android:textSize="11sp" />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_marginStart="6dip"
                android:layout_marginTop="3dip"
                android:layout_marginEnd="6dip"
                android:layout_marginBottom="3dip"
                android:background="@color/white" />

            <TextView
                android:id="@+id/btnPrivacyInfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:text="@string/btn_privacy_info_text"
                android:textColor="@color/white"
                android:textSize="11sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingStart="42dp"
            android:paddingTop="28dp"
            android:paddingBottom="6dp">

            <TextView
                android:id="@+id/btnBizName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="12dp"
                android:singleLine="true"
                android:textColor="@color/white"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:text="리치빔" />

            <ImageView
                android:id="@+id/btnBizView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:layout_toRightOf="@+id/tvLocation"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/color_757575"
            android:orientation="vertical"
            android:paddingStart="5dp"
            android:paddingTop="10dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.24"
                    android:gravity="right"
                    android:singleLine="true"
                    android:text="@string/lbl_biz_ceo"
                    android:textAlignment="gravity"
                    android:textColor="@color/color_bfbfbf"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_weight="0.68"
                    android:singleLine="true"
                    android:textColor="@color/color_dddbd7"
                    android:textSize="12sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.24"
                    android:gravity="right"
                    android:singleLine="true"
                    android:text="@string/lbl_biz_address"
                    android:textAlignment="gravity"
                    android:textColor="@color/color_bfbfbf"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_weight="0.68"
                    android:singleLine="true"
                    android:textColor="@color/color_dddbd7"
                    android:textSize="12sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.24"
                    android:gravity="right"
                    android:singleLine="true"
                    android:text="@string/lbl_biz_no"
                    android:textAlignment="gravity"
                    android:textColor="@color/color_bfbfbf"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_weight="0.68"
                    android:singleLine="true"
                    android:textColor="@color/color_dddbd7"
                    android:textSize="12sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.24"
                    android:gravity="right"
                    android:singleLine="true"
                    android:text="@string/lbl_biz_sales_"
                    android:textAlignment="gravity"
                    android:textColor="@color/color_bfbfbf"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tvComm"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_weight="0.68"
                    android:singleLine="true"
                    android:textColor="@color/color_dddbd7"
                    android:textSize="12sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.24"
                    android:gravity="right"
                    android:singleLine="true"
                    android:text="@string/lbl_biz_hsp"
                    android:textAlignment="gravity"
                    android:textColor="@color/color_bfbfbf"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tvHost"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_weight="0.68"
                    android:singleLine="true"
                    android:textColor="@color/color_dddbd7"
                    android:textSize="12sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="10dp"
                android:paddingBottom="10dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:lineSpacingExtra="7sp"
                    android:text="@string/info_biz_call_center"
                    android:textColor="@color/color_fcc21b"
                    android:textSize="12sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.24"
                    android:gravity="right"
                    android:singleLine="true"
                    android:text="@string/lbl_biz_inquiry"
                    android:textAlignment="gravity"
                    android:textColor="@color/color_bfbfbf"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_weight="0.68"
                    android:singleLine="true"
                    android:textColor="@color/color_dddbd7"
                    android:textSize="12sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingBottom="10dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.23"
                    android:gravity="right"
                    android:singleLine="true"
                    android:text="@string/lbl_biz_call_center"
                    android:textAlignment="gravity"
                    android:textColor="@color/color_bfbfbf"
                    android:textSize="12sp" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.68"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:singleLine="true"
                        android:textColor="@color/color_dddbd7"
                        android:textSize="12sp"
                        android:textStyle="bold"
                        tools:text="02-2043-0074" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:singleLine="true"
                        android:text="@string/txt_biz_call_center"
                        android:textColor="@color/color_dddbd7"
                        android:textSize="12sp" />
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingStart="10dp"
            android:paddingTop="9dp"
            android:paddingBottom="32dp">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/info_biz_message"
                android:textColor="@color/color_3c3c3c"
                android:textSize="12sp" />

        </LinearLayout>

    </LinearLayout>

</layout>

